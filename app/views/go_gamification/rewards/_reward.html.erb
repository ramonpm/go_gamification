<% if reward.medal %>
        <div class="col-xs-6">
          <div class="push-5"><%= image_tag reward.medal.image.icon32 %></div>
          <div class="h5 font-w300 text-muted"><%= reward.medal.name %> - <%= reward.medal.description %></div>
        </div>
<% end %>

<div class="points"><%= reward.points %></div>

